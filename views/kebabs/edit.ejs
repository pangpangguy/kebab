<form action="/kebabs/<%= kebab._id %>?_method=PUT" method="post">
  <label for="name">Name:</label>
  <input type="text" name="kebab[name]" id="name" value="<%= kebab.name %>" />
  <label for="location">Location:</label>
  <input
    type="text"
    name="kebab[location]"
    id="location"
    value="<%=kebab.location%>"
  />
  <button>Submit Change</button>
</form>
